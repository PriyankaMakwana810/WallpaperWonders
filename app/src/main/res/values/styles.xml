<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Theme.CustomSplashScreenTheme" parent="Theme.SplashScreen">
        <item name="colorPrimary">@color/blue_shade</item>
        <item name="colorPrimaryVariant">@color/blue_shade</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="colorAccent">@color/blue</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/blue_shade</item>
        <item name="colorSecondaryVariant">@color/blue_shade</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <item name="windowSplashScreenBackground">@color/blue_shade</item>
        <item name="windowSplashScreenAnimatedIcon">@mipmap/ic_launcher_round</item>
        <item name="windowSplashScreenAnimationDuration">300</item>
        <item name="postSplashScreenTheme">@style/Theme.BriefCaseSampleApp</item>
    </style>



